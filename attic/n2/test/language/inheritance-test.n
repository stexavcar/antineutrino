protocol A {
  def x() -> 10;
  def z() -> 11;
}

protocol B : A {
  def x() -> 12;
  def y() -> 13;
}

protocol C : B {
  def y() -> 14;
}

def method_inheritance_test() {
  def a := new A();
  assert a.x() = 10;
  assert a.z() = 11;
  def b := new B();
  assert b.x() = 12;
  assert b.y() = 13;
  assert b.z() = 11;
  def c := new C();
  assert c.x() = 12;
  assert c.y() = 14;
  assert c.z() = 11;
}

protocol AS {
  static def x() -> 10;
  static def z() -> 11;
}

protocol BS : AS {
  static def x() -> 12;
  static def y() -> 13;
}

protocol CS : BS {
  static def y() -> 14;
}

def static_inheritance_test() {
  assert AS.x() = 10;
  assert AS.z() = 11;
  assert BS.x() = 12;
  assert BS.y() = 13;
  assert BS.z() = 11;
  assert CS.x() = 12;
  assert CS.y() = 14;
  assert CS.z() = 11;
}

def main() {
  method_inheritance_test();
  static_inheritance_test();
}
