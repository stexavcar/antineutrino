def simple_list_test() {
  def l := new List();
  l.add(1);
  l.add(2);
  l.add(3);
  assert l[0] = 1;
  assert l[1] = 2;
  assert l[2] = 3;
}

def extend_test() {
  def l := new List();
  for (i in 0 .. 100) {
    assert |l| = i;
    for (j in 0 .. i)
      assert l[j] = j;
    l.add(i);
  }
}

def main() {
  simple_list_test();
  extend_test();
}
