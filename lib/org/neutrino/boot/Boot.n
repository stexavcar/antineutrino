@entry_point
def main() {
  test_integer_operators();
  test_singletons();
  test_conditionals();
}

def test_integer_operators() {
  assert_true((1 + 1) = 2);
  assert_true((1 + 2) = 3);
  assert_true((2 + 1) = 3);
  assert_true(1 < 2);
}

def test_singletons() {
  assert_true(true);
  assert_true(false.not());
}

def test_conditionals() {
  print(if true then "knirk" else "knork");
}
