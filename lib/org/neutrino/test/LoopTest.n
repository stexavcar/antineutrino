def test_loops() {
  print("Testing loops");
  def i := ref(0);
  def x := with_1cc break -> loop(fn () {
    if i.get() = 10 then {
      break.fire("k")
    } else {
      i.set(i.get() + 1);
    }
  });
  assert_equal(10, i.get());
  assert_equal("k", x);
  def y := with_1cc break -> 5;
  assert_equal(5, y);
}
