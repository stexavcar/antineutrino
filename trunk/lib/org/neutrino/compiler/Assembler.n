protocol Operand;

protocol Register is Operand;

protocol Memory is Operand;

def Memory.at(base) -> new Memory(base, 0);

def Memory.at_disp(base, disp) -> new Memory(base, disp);

protocol CallTarget;

protocol LocalCallTarget;

def CallTarget.local(name) -> new LocalCallTarget { name := name };

protocol ExternalCallTarget;

def CallTarget.extern(name) -> new ExternalCallTarget { name := name };
