def quote_test() {
  <1 + 1 + 1>;
  assert_equals("#<class Boolean>", Boolean.to_string());
  assert_equals("#<class Foo>", <class Foo { }>.evaluate().to_string());
  assert_equals("#<class Foo>", <class Foo { def x() -> 4; }>.evaluate().to_string());
  print(new (<class Foo { def x() -> 4; }>.evaluate())());
}

def main() {
  quote_test();
}
