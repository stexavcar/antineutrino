def simple_record_test() {
  def x := new { a: 0, b: 1, c: 2 };
  assert_equals(0, x.a());
  assert_equals(1, x.b());
  assert_equals(2, x.c());
  def y := new { c: "x", b: "y", a: "z" };
  assert_equals("z", y.a());
  assert_equals("y", y.b());
  assert_equals("x", y.c());
}

def main() {
  simple_record_test();
}
