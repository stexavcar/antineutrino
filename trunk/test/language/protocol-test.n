protocol A {
  def x() -> 4;
}

protocol B : A {
  def x() -> 7;
}

def test_inheritance() {
  def a := new A();
  def b := new B();
  assert_equals(4, a.x());
  assert_equals(7, b.x());
}

protocol X {
  static def foo() -> 8;
}

def test_statics() {
  assert_equals(8, X.foo());
}

def main() {
  test_inheritance();
  test_statics();
}
