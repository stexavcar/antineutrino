def simple_list_test() {
  def l := new List();
  l.add(1);
  l.add(2);
  l.add(3);
  assert l[0] = 1;
  assert l[1] = 2;
  assert l[2] = 3;
}

def extend_test() {
  def l := new List();
  var i := 0;
  while (i < 100) {
    assert |l| = i;
    var j := 0;
    while (j < i) {
      assert l[j] = j;
      j := j + 1;
    }
    l.add(i);
    i := i + 1;
  }
}

def main() {
  simple_list_test();
  extend_test();
}
