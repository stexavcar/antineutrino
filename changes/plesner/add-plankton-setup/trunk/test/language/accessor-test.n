def test_getter() {
  def x := new { a: 1, b: 2, c: 3 };
  assert x.a = 1;
  assert x.b = 2;
  assert x.c = 3;  
}

def test_setter() {
  def x := new { a: 1, b: 2, c: 3 };
  assert x.a = 1;
  assert x.b = 2;
  assert x.c = 3;
  x.a := 4;
  assert x.a = 4;
  assert x.b = 2;
  assert x.c = 3;  
  x.b := 5;
  assert x.a = 4;
  assert x.b = 5;
  assert x.c = 3; 
  x.c := 6;
  assert x.a = 4;
  assert x.b = 5;
  assert x.c = 6;
  x.a := 7;
  assert x.a = 7;
  assert x.b = 5;
  assert x.c = 6;  
  x.b := 8;
  assert x.a = 7;
  assert x.b = 8;
  assert x.c = 6; 
  x.c := 9;
  assert x.a = 7;
  assert x.b = 8;
  assert x.c = 9;
}

def main() {
  test_getter();
  test_setter();
}
