protocol Interval {

  def | | -> (this.to - this.from);
  
  def [index] -> index + this.from;
  
  def for(f) {
    var i := this.from;
    while (i < this.to) {
      f(i);
      i := i + 1;
    }
  }

  def to_string() -> "[${this.from}..${this.to}]";

  static def new(from:, to:) -> new Interval() { from: from, to: to };

}
